<+><h1>Homepage</h1>\n<p>this is the mainpage displaying todays options</p>\n\n\n<!-- header -->\n<h2> Logo </h2>\n<h2><%= link_to 'All Meals', meals_path %></h2>\n<h2><%= link_to 'Groups', groups_path %></h2>\n<h2><%= link_to 'New Meal', new_meal_path %></h2>\n<h2><%= link_to 'My Lunch', dashboard_index_url %></h2>\n\n<!-- header end -->\n<br>\n<br>\n<br>\n<!-- body -->\n<table>\n  <tr>\n    <th>Rank</th>\n    <th>Title</th>\n    <th>Member</th>\n    <th>Tags</th>\n    <th>Upvotes</th>\n  </tr>\n\n  <% @meals.each do | meal |  %>\n    <tr>\n\n      <th>1</th>\n      <td><%= link_to meal.title, meal %></td>\n      <td><%= link_to meal.group, meal %></td>\n      <td><%= link_to meal.tags, meal %></td>\n      <td><%= link_to meal.upvotes, meal %></td>\n\n    </tr>\n  <% end %>\n</table>\n
